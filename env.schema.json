{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": [
    "HOGE_INTEGER"
  ],
  "properties": {
    "HOGE_STRING": {
      "description": "パターン指定無しの文字列だよ"
    },
    "HOGE_INTEGER": {
      "description": "整数値だよ",
      "pattern": "^[0-9]+$"
    },
    "DEBUG_MODE": {
      "description": "デバッグ出力を有効にする",
      "enum": ["true", "false"],
      "default": "false"
    }
  }
}